<?xml version="1.0" encoding="UTF-8" ?>
<XMLDB PATH="local/oevents/db" VERSION="20140819" COMMENT="XMLDB file for Moodle local/oevents"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:noNamespaceSchemaLocation="../../../lib/xmldb/xmldb.xsd"
>
  <TABLES>
    <TABLE NAME="local_oevents" COMMENT="Default comment for local_oevents, please edit me">
      <FIELDS>
        <FIELD NAME="id" TYPE="int" LENGTH="10" NOTNULL="true" SEQUENCE="true"/>
      </FIELDS>
      <KEYS>
        <KEY NAME="primary" TYPE="primary" FIELDS="id"/>
      </KEYS>
    </TABLE>
    <TABLE NAME="course_ms_ext" COMMENT="Extension for adding up the calendar id and section id to course ">
      <FIELDS>
        <FIELD NAME="id" TYPE="int" LENGTH="10" NOTNULL="true" SEQUENCE="true"/>
        <FIELD NAME="course_id" TYPE="int" LENGTH="20" NOTNULL="false" SEQUENCE="false" COMMENT="Course id created by moodle"/>
        <FIELD NAME="calendar_id" TYPE="char" LENGTH="300" NOTNULL="false" SEQUENCE="false" COMMENT="Course id created by office calendar which is passes to the moodle"/>
        <FIELD NAME="section_id" TYPE="char" LENGTH="300" NOTNULL="false" SEQUENCE="false" COMMENT="Section id created by office OneNote which is passes to the moodle"/>
      </FIELDS>
      <KEYS>
        <KEY NAME="primary" TYPE="primary" FIELDS="id"/>
      </KEYS>
    </TABLE>
  </TABLES>
</XMLDB>